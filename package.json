{
  "name": "Bloxby",
  "version": "1.4.1",
  "private": true,
  "engines": {
    "node": "6.12.2"
  },
  "scripts": {
    "build": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --optimize-minimize --mode=production",
    "start": "NODE_ENV=development ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot-inline --mode=development",
    "build-with-sourcemaps": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --optimize-minimize --mode=production --env.sourcemaps=true",
    "start-with-sourcemaps": "NODE_ENV=development ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot-inline --mode=development --env.sourcemaps=true",
    "winbuild": "set NODE_ENV=production && webpack --optimize-minimize --mode=production",
    "winstart": "set NODE_ENV=development && webpack-dev-server --hot-inline --mode=development"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.2.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-check-es2015-constants": "^6.22.0",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.0.0",
    "file-loader": "^2.0.0",
    "img-loader": "^3.0.1",
    "jshint": "^2.10.1",
    "jshint-loader": "git+https://github.com/webpack-contrib/jshint-loader.git",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "script-loader": "^0.7.0",
    "speed-measure-webpack-plugin": "^1.2.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.2.0",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "dependencies": {
    "bootstrap-select": "^1.13.5",
    "brace": "^0.11.1",
    "detect-element-resize": "0.0.2",
    "jquery-hoverintent": "^1.8.2",
    "jquery-lazyload": "^1.9.7",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "shortid": "^2.2.14"
  }
}
